@model Stock

<form class="form-inline" asp-controller="Stock" asp-action="Edit" method="post" enctype="multipart/form-data">
  <div class="form-group mb-2">
    <input type="text" asp-for="Quantity" value="@Model.Quantity" >
  </div>



  <button type="submit" class="btn btn-primary mb-2">Edit Stock</button>
</form>
