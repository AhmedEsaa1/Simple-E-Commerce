@model IEnumerable<Stock>

@{
    foreach (var item in Model)
    {
        <h3>Id: @item.Id</h3>
        <h3>Quantity: @item.Quantity</h3>
        <h3>Created at: @item.Created_at</h3>

        <a class="btn btn-outline-dark mt-auto" asp-action="Delete" asp-controller="Stock" asp-route-id="@item.Id">Delete</a>
        <a class="btn btn-outline-dark mt-auto" asp-action="Edit" asp-controller="Stock" asp-route-id="@item.Id">Edit</a>

        <br><hr>

    }
}